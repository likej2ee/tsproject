{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": [
          "> 1%",
          "last 2 versions",
          "safari >= 7",
          "not ie <= 8"
        ]
      }
    }],
    "stage-2"
  ],
  "plugins": [
    "transform-strict-mode",
    "transform-vue-jsx",
    "jsx-v-model",
    "syntax-dynamic-import"
  ]
}
